<%= form_with url: "/warehouses", method: :post, local: true do |f| %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.label :city %>
  <%= f.text_field :city %>
  <%= f.label :state %>
  <%= f.text_field :state %>
  <%= f.label :country %>
  <%= f.text_field :country %>
  <%= f.label :postal_code %>
  <%= f.text_field :postal_code %>

  <%= f.submit "Create Warehouse" %>
<% end %>
